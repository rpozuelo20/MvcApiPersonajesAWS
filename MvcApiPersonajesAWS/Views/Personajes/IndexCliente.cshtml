@section scripts{ 
    <script>
        $(document).ready(function () {
            $("#btnLoad").click(function () {
                var url = "https://44.205.0.194/";
                $.get(url + "api/personajes", function (data) {
                    var html = "";
                    $.each(data, function (index, personaje) {
                        html += "<tr>";
                        html += "<td>" + personaje.nombre + "</td>";
                        html += "<td class='text-center'>";
                        html += "<img src='" + personaje.imagen + "' style='width:350px;height:250px;object-fit:cover;'/>";
                        html += "</td>";
                        html += "</tr>";
                    });
                    $("#tblPersonajes tbody").html(html);
                });
            });
        });
    </script>
}
<div class="container">
    <h1>AWS RDS Api Personajes</h1>
    <button type="button" id="btnLoad" class="btn btn-info">Cargar Personajes</button>
    <table class="table table-bordered" id="tblPersonajes">
        <thead>
            <tr>
                <th>Personaje</th>
                <th>Imagen</th>
            </tr>
        </thead>
        <tbody>

        </tbody>
    </table>
</div>